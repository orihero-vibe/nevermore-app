{
  "expo": {
    "name": "nevermore-app",
    "slug": "nevermore-app",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./src/assets/bell.png",
    "splash": {
      "image": "./src/assets/splash-bg.png",
      "resizeMode": "cover",
      "backgroundColor": "#2D1B69"
    },
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "scheme": "nevermoreapp",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nevermore.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/bell.png",
        "backgroundColor": "#2D1B69"
      },
      "package": "com.nevermoreapp"
    },
    "web": {
      "favicon": "./src/assets/bell.png"
    },
    "plugins": [
      "expo-asset",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#2D1B69"
        }
      ]
    ]
  }
}
